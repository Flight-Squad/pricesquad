    "axios": "^0.19.0",
    "c8": "^5.0.4",
    "cheerio": "^1.0.0-rc.3",
    "dotenv": "^8.1.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "module-alias": "^2.2.1",
    "morgan": "^1.9.1",
    "nightmare": "^3.0.2",
    "typescript": "^3.6.3",
    "winston": "^3.2.1"
  },
  "_moduleAliases": {
